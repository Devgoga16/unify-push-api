# Variables de entorno para desarrollo
NODE_ENV=development
PORT=3000

# Configuración de MongoDB
MONGODB_URI=mongodb://localhost:27017/unifypush

# Configuración JWT - IMPORTANTE: Cambia esto por un valor único y seguro en producción
JWT_SECRET=CHANGE_THIS_JWT_SECRET_IN_PRODUCTION_USE_A_LONG_RANDOM_STRING_128_CHARS_MINIMUM
JWT_EXPIRE=30d

# Configuración de la aplicación
API_VERSION=v1